{
  "name": "vfiles",
  "version": "1.0.0",
  "description": "基于Web的Git文件管理系统",
  "type": "module",
  "scripts": {
    "dev": "bun run --watch server/src/index.ts",
    "dev:client": "cd client && bun run dev",
    "build": "cd client && bun run build",
    "start": "bun run server/src/index.ts",
    "lint": "eslint .",
    "fmt": "prettier --write .",
    "test": "bun test"
  },
  "keywords": [
    "git",
    "file-manager",
    "vue3",
    "bulma"
  ],
  "author": "",
  "license": "MIT",
  "dependencies": {
    "fflate": "^0.8.2",
    "hono": "^4.11.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.39.2",
    "@types/node": "^25.0.3",
    "@typescript-eslint/eslint-plugin": "^8.51.0",
    "@typescript-eslint/parser": "^8.51.0",
    "bun-types": "^1.3.5",
    "eslint": "^9.39.2",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-vue": "^10.6.2",
    "globals": "^17.0.0",
    "prettier": "^3.7.4",
    "typescript": "^5.9.3",
    "vue-eslint-parser": "^10.2.0"
  }
}
